{
  "name": "paolooo/laravel-doctrine",
  "description": "Doctrine 2 for Laravel 5 Artisans",
  "version": "0.4.1",
  "keywords": [
    "doctrine",
    "laravel",
    "orm"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nino Paolo Amarillento",
      "email": "oo70vd@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "5.0.*",
    "doctrine/orm": "~2.4",
    "php-daddy/contracts": "*",
    "php-daddy/contract-singleton": "*"
  },
  "autoload": {
    "psr-4": {
      "Paolooo\\LaravelDoctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "Paolooo\\Test\\": "tests",
        "Paolooo\\Stub\\": "stub",
        "Paolooo\\Acme\\": "examples/Acme"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.4.*-dev"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "mockery/mockery": "0.9.*@dev"
  }
}
